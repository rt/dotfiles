{
  "name": "server",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "lint": "eslint src/models src/data src/utils",
    "cover": "nyc --reporter=html npm run test && nyc report",
    "test": "GOOGLE_APPLICATION_CREDENTIALS='../key.json' mocha --require @babel/register 'src/utils/test_setup.js' 'src/**/*.spec.js'",
    "test:watch": "npm run test -- --watch",
    "clean": "babel-node tools/clean",
    "copy": "babel-node tools/copy",
    "bundle": "babel-node tools/bundle",
    "build": "npm run clean && npm run copy && npm run bundle",
    "start": "NODE_ENV=development GOOGLE_APPLICATION_CREDENTIALS='../key.json'  node --inspect build/server.js",
    "create-admin": "GOOGLE_APPLICATION_CREDENTIALS='../key.json' babel-node tools/create-admin",
    "build-docker": "npm run build && docker build -t betsy ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ryan_tsunoda/betsy.git"
  },
  "author": "Ryan Tsunoda",
  "license": "MIT",
  "homepage": "https://bitbucket.org/ryan_tsunoda/betsy#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "@babel/polyfill": "^7.2.5",
    "@google-cloud/datastore": "^3.0.1",
    "@google-cloud/storage": "^2.4.2",
    "accounting": "0.4.1",
    "body-parser": "^1.18.3",
    "config": "1.26.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-alpha.2",
    "csurf": "1.9.0",
    "deepmerge": "2.0.1",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-session": "1.15.6",
    "history": "4.4.0",
    "humps": "2.0.0",
    "jsonwebtoken": "7.1.9",
    "kabuda-liquid": "^0.4.10",
    "moment": "2.23.0",
    "multer": "^0.1.8",
    "node-fetch": "^2.1.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "6.4.2",
    "pg-camelcase": "0.0.2",
    "pretty-error": "^2.1.1",
    "query-string": "^6.1.0",
    "source-map-support": "0.4.6",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "chai": "^3.5.0",
    "chai-as-promised": "7.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "gaze": "^1.1.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.0",
    "nyc": "11.0.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "watch": "^1.0.1",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-node-externals": "^1.7.2"
  }
